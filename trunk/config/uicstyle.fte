# C indentation style menu

menu MChangeCIndentStyle {
    item "&Default" {
        8 ChangeTabSize 8;
        0 SetIndentWithTabs 0
        4 0 -1 0 4 0 4 -4 0 1 -1 4 4 SetCIndentStyle
    }
    item "&Linux kernel" {
        8 ChangeTabSize;
        1 SetIndentWithTabs
        8 0 -1 0 8 0 8 -8 0 1 -1 8 8 SetCIndentStyle
    }
    item "&GNU" {
        8 ChangeTabSize;
        1 SetIndentWithTabs
        2 2 -1 0 2 0 2 -2 0 1 -1 2 2 SetCIndentStyle
    }
}
